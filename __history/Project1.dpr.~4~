program Project1;

uses
  Vcl.Forms,
  Unit1 in 'Unit1.pas' {Form1},
  UnitGame_Over in 'UnitGame_Over.pas' {FormGame_Over},
  nft in 'nft.pas' {FormRecords},
  UnitMakers in 'UnitMakers.pas' {FormMakers};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TFormGame_Over, FormGame_Over);
  Application.CreateForm(TFormRecords, FormRecords);
  Application.CreateForm(TFormMakers, FormMakers);
  Application.Run;
end.
